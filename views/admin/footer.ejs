    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const sidebar = document.getElementById('sidebar');
            const toggleBtn = document.getElementById('toggleSidebar');
            const mobileMenuToggle = document.getElementById('mobileMenuToggle');
            const content = document.getElementById('content');
            
            // Toggle sidebar function
            function toggleSidebar() {
                if (window.innerWidth <= 767.98) {
                    sidebar.classList.toggle('hidden');
                    content.classList.toggle('full-width');
                    
                    if (sidebar.classList.contains('hidden')) {
                        toggleBtn.classList.add('sidebar-hidden');
                    } else {
                        toggleBtn.classList.remove('sidebar-hidden');
                    }
                } else {
                    sidebar.classList.toggle('expanded');
                    if (sidebar.classList.contains('expanded')) {
                        toggleBtn.innerHTML = '<i class="bi bi-chevron-left"></i>';
                    } else {
                        toggleBtn.innerHTML = '<i class="bi bi-chevron-right"></i>';
                    }
                }
            }
            
            // Toggle sidebar event listeners
            toggleBtn.addEventListener('click', toggleSidebar);
            if (mobileMenuToggle) {
                mobileMenuToggle.addEventListener('click', toggleSidebar);
            }
            
            // Handle tab navigation
            const tabLinks = document.querySelectorAll('a[data-bs-toggle="tab"]');
            tabLinks.forEach(link => {
                link.addEventListener('click', function() {
                    // If on mobile, close the sidebar after clicking a tab
                    if (window.innerWidth <= 767.98) {
                        sidebar.classList.add('hidden');
                        content.classList.add('full-width');
                        toggleBtn.classList.add('sidebar-hidden');
                    }
                    
                    // Update active states for sidebar items
                    document.querySelectorAll('.sidebar-menu > li').forEach(item => {
                        item.classList.remove('active');
                    });
                    
                    const parentLi = this.closest('li');
                    if (parentLi && !parentLi.classList.contains('has-submenu')) {
                        parentLi.classList.add('active');
                    }
                });
            });
            
            // Handle dropdown menus - completely rewritten for simplicity
            document.querySelectorAll('.has-submenu > a').forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    
                    const parentLi = this.parentElement;
                    const submenu = parentLi.querySelector('.sidebar-submenu');
                    const dropdownIcon = this.querySelector('.dropdown-icon');
                    
                    // Close all other submenus
                    document.querySelectorAll('.has-submenu').forEach(item => {
                        if (item !== parentLi) {
                            item.classList.remove('open');
                            const otherSubmenu = item.querySelector('.sidebar-submenu');
                            if (otherSubmenu) {
                                otherSubmenu.style.display = 'none';
                            }
                            const otherIcon = item.querySelector('.dropdown-icon');
                            if (otherIcon) {
                                otherIcon.style.transform = 'rotate(0)';
                            }
                        }
                    });
                    
                    // Toggle current submenu
                    parentLi.classList.toggle('open');
                    if (parentLi.classList.contains('open')) {
                        submenu.style.display = 'block';
                        if (dropdownIcon) {
                            dropdownIcon.style.transform = 'rotate(90deg)';
                        }
                    } else {
                        submenu.style.display = 'none';
                        if (dropdownIcon) {
                            dropdownIcon.style.transform = 'rotate(0)';
                        }
                    }
                });
            });
            
            // Mobile dropdown handlers
            document.querySelectorAll('.mobile-dropdown').forEach(dropdown => {
                dropdown.addEventListener('click', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    
                    const parentLi = this.parentElement;
                    const submenu = parentLi.querySelector('.sidebar-submenu');
                    const icon = this.querySelector('i');
                    
                    // Close other submenus
                    document.querySelectorAll('.has-submenu').forEach(item => {
                        if (item !== parentLi) {
                            item.classList.remove('open');
                            const otherSubmenu = item.querySelector('.sidebar-submenu');
                            if (otherSubmenu) {
                                otherSubmenu.style.display = 'none';
                            }
                            const otherIcon = item.querySelector('.mobile-dropdown i');
                            if (otherIcon) {
                                otherIcon.style.transform = 'rotate(0)';
                            }
                        }
                    });
                    
                    // Toggle current submenu
                    parentLi.classList.toggle('open');
                    if (parentLi.classList.contains('open')) {
                        submenu.style.display = 'block';
                        if (icon) {
                            icon.style.transform = 'rotate(90deg)';
                        }
                    } else {
                        submenu.style.display = 'none';
                        if (icon) {
                            icon.style.transform = 'rotate(0)';
                        }
                    }
                });
            });
            
            // Handle window resize
            window.addEventListener('resize', function() {
                // Update mobile dropdown visibility
                const isExpanded = sidebar.classList.contains('expanded');
                const isDesktop = window.innerWidth > 991.98;
                
                document.querySelectorAll('.mobile-dropdown').forEach(dropdown => {
                    dropdown.style.display = (isExpanded || isDesktop) ? 'none' : 'flex';
                });
                
                document.querySelectorAll('.dropdown-icon').forEach(icon => {
                    icon.style.display = (isExpanded || isDesktop) ? 'block' : 'none';
                });
                
                // Ensure proper sidebar state on desktop
                if (window.innerWidth > 991.98) {
                    sidebar.classList.add('expanded');
                    toggleBtn.innerHTML = '<i class="bi bi-chevron-left"></i>';
                } else if (window.innerWidth <= 991.98 && window.innerWidth > 767.98) {
                    sidebar.classList.remove('expanded');
                    toggleBtn.innerHTML = '<i class="bi bi-chevron-right"></i>';
                }
            });
            
            // Handle logout button
            const logoutBtn = document.getElementById('logoutBtn');
            const mobileLogoutBtn = document.getElementById('mobileLogoutBtn');
            
            function handleLogout(e) {
                e.preventDefault();
                alert('Logout functionality would go here.');
            }
            
            if (logoutBtn) logoutBtn.addEventListener('click', handleLogout);
            if (mobileLogoutBtn) mobileLogoutBtn.addEventListener('click', handleLogout);
            
            // Set initial sidebar state based on screen size
            if (window.innerWidth > 991.98) {
                sidebar.classList.add('expanded');
                toggleBtn.innerHTML = '<i class="bi bi-chevron-left"></i>';
            } else {
                sidebar.classList.remove('expanded');
                toggleBtn.innerHTML = '<i class="bi bi-chevron-right"></i>';
                
                // Update dropdown visibility for mobile
                document.querySelectorAll('.mobile-dropdown').forEach(dropdown => {
                    dropdown.style.display = 'flex';
                });
                
                document.querySelectorAll('.dropdown-icon').forEach(icon => {
                    icon.style.display = 'none';
                });
            }
            
            // Trigger resize event to set initial states
            window.dispatchEvent(new Event('resize'));
        });
    </script>

